<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzhb.trainTicket.IStudentDao">
    <insert id="insertStudent" parameterType="com.xzhb.trainTicket.Student">
        insert into student(NO,name) values(#{no},#{name})
    </insert>
    <select id="getStudent" parameterType="String" resultType="com.xzhb.trainTicket.Student">
        SELECT NO,name FROM  student WHERE NO=#{no}
    </select>
</mapper>